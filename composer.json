{
    "name": "nunomaduro/laravel-mojito",
    "description": "A lightweight package for testing Laravel views.",
    "keywords": [
        "php",
        "laravel",
        "package",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "symfony/css-selector": "^5.1|^6.0",
        "symfony/dom-crawler": "^5.1.8|^6.0"
    },
    "require-dev": {
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/view": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^0.12.11",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/bootstrap.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NunoMaduro\\LaravelMojito\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test:styles": "phpcs",
        "test:types": "phpstan analyse --ansi",
        "test:unit": "phpunit --colors=always",
        "test": [
            "@test:styles",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NunoMaduro\\LaravelMojito\\MojitoServiceProvider"
            ]
        }
    }
}
